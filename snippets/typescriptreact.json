{
  "scmp (Simple Component)": {
    "prefix": "scmp (Simple Component)",
    "body": ["export const ${1:ComponentName} = () => {", "  $0", "}"]
  },
  "dcn (Div Class Name)": {
    "prefix": "dcn (Div Class Name)",
    "body": "<div className=\"$1\">$0</div>"
  },
  "ddr (Double div return)": {
    "prefix": "ddr (Double div return)",
    "body": ["return (", "  <div>", "    <div>$0</div>", "  </div>", ");"]
  },
  "rscp (React server component page)": {
    "prefix": "rscp (React server component page)",
    "body": [
      "export default async function ${1:Page}() {",
      "  return (",
      "    <div>",
      "      <div>$0</div>",
      "    </div>",
      "  )",
      "}"
    ]
  },
  "ret (Return)": {
    "prefix": "ret (Return)",
    "body": ["  return (", "    $0", "  );"]
  }
}
